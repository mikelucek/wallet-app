
<ul>
<% @users.each do |x| %>
	<li> user:
		<%= x.fname %> <%= x.lname %>
		<ul>
			<% if x.cards.count < 1 %>
				<li> No cards. </li>
			<% else %>
			<% x.cards.each do |y| %>
			<li> 
				<%= image_tag("/" + y.ctype + ".png", width: "120") %>
				<%= y.cnumber %>
				<% if x.id == session[:id] %>
				<%= link_to "share or unshare", (relationship_path y) %>
				<% end %>
			</li>
			<% end %>
			<% end %>
		</ul>
	</li>

<% end %>
</ul>